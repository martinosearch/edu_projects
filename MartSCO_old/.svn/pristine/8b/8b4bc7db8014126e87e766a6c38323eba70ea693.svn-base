package componentFactory;

import java.awt.Color;
import java.awt.Font;

import javax.swing.JMenu;

public class MartMenu extends JMenu {
	private Font font = new Font("Arial", Font.BOLD, 12);
	public static Color FORE_COLOR = new Color(252, 207, 174);

	public MartMenu(String str) {
		super(str);
		setFont(font);
		setForeground(FORE_COLOR);
	}
}
