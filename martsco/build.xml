            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/jxl.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/lombok.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/opencsv-3.6.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/poi-3.17.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/poi-excelant-3.17.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/poi-ooxml-3.17.jar"/>
