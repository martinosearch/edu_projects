            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/postgresql-9.4-1201.jdbc41.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/jlatexmath-1.0.3.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/jlatexmath-embedded-fop-1.0.3.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/jlatexmath-fop-1.0.3.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/pswing.jar"/>
        </jar>
    </target>
</project>
